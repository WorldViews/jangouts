{
  "globalstrict": true,
  "bitwise": true,
  "curly": true,
  "eqeqeq": true,
  "immed": true,
  "indent": 2,
  "latedef": false,
  "newcap": true,
  "noarg": true,
  "regexp": true,
  "undef": true,
